<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <carriers>
            <ingrid>
                <active>0</active>
                <api_url>https://api.ingrid.com</api_url>
                <stage_api_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <prod_api_key backend_model="Magento\Config\Model\Config\Backend\Encrypted" />
                <test_mode>1</test_mode>
                <model>Ingrid\Checkout\Model\Carrier</model>
                <sort_order></sort_order>
                <fallback_id>ingrid_fallback</fallback_id>
                <fallback_name>Delivery</fallback_name>
            </ingrid>
        </carriers>
    </default>
</config>
